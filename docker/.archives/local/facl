# file: /
# owner: root
# group: root
user::rwx
group::r-x
other::r-x

# file: /etc
# owner: root
# group: root
user::rwx
group::r-x
other::r-x

# file: /etc/docker
# owner: root
# group: root
user::rwx
group::r-x
other::r-x

# file: /etc/docker/daemon.json
# owner: root
# group: root
user::rw-
group::r--
other::r--

